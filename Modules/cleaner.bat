@echo off
Title Clean Up Tool
color 04
mode 50,20
GOTO menu



:menu
cls

	echo.
	echo ---------------------------
	echo PC Cleaner                     
	echo ---------------------------
	echo By Thomas Reddish
	echo. 
	echo 1. (ADMIN)Defrag your PC
	echo 2. Clear Cookies
	echo 3. Clean Disk
	echo 4. Clear internet Temp Files
	echo 5. Clear Recycle Bin
	echo 6. ALL
	echo Q. Quit
	echo.
	choice /c:1234560q /M "Please choose an action: "
	echo.

	if %errorlevel%==1 call :defrag
	if %errorlevel%==2 call :cookies
	if %errorlevel%==3 call :disk
	if %errorlevel%==4 call :temps
	if %errorlevel%==5 call :bin
	if %errorlevel%==6 call :all
	if %errorlevel%==7 call
	if %errorlevel%==8 call
	if %errorlevel%==0 call :quit

:all
cls
ping 127.0.0.1 > nul
echo ---------------------------
echo Delete Internet Cookies
echo ---------------------------
echo.
echo Deleting Cookies...
ping localhost -n 3 >nul
del /f /q "%userprofile%\Cookies\*.*"
cls
echo ---------------------------
echo Cookies deleted
echo ---------------------------
echo.
echo You cant hold the cookie monster back...
echo.
ping 127.0.0.1 > nul
cls

